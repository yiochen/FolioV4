{
  "name": "yiou-blog",
  "description": "Yiou's blog",
  "private": true,
  "version": "0.0.0",
  "author": "Yiou Chen <yioucc@gmail.com>",
  "dependencies": {
    "babel-plugin-polished": "^1.1.0",
    "babel-preset-gatsby": "^0.1.8",
    "core-js": "^2.6.5",
    "disqus-react": "^1.0.5",
    "gatsby": "^2.1.19",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-linaria": "^1.0.2",
    "gatsby-plugin-polished": "^1.0.3",
    "gatsby-plugin-react-helmet": "^3.0.7",
    "gatsby-remark-prismjs": "^3.0.3",
    "gatsby-source-contentful": "^2.0.33",
    "gatsby-transformer-remark": "^2.3.0",
    "linaria": "^1.2.4",
    "lodash": "^4.17.11",
    "polished": "^3.0.3",
    "prismjs": "^1.15.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "yiou-css-base": "^0.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "rm -rf ./public",
    "build": "npm run clean && npm run build:dev",
    "build:dev": "gatsby build",
    "serve": "gatsby serve",
    "dev": "npm run clean && gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint .",
    "test": "echo \"Error: I only test in production\" && exit 1"
  },
  "devDependencies": {
    "eslint": "^5.15.0",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.15.2"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  }
}
